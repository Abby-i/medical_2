<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <!--中文乱码-->
    <filter>
        <filter-name>CharacterEncodingFilter</filter-name>
        <filter-class>com.gxuwz.medical_2.web.filter.CharacterEncodingFilter</filter-class>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>CharacterEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <!--servlet-->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoleServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.RoleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MenuServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.MenuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AreaServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.AreaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FamilyArchivesServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.FamilyArchivesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountArchivesServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.AccountArchivesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RuralCooperativeServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.RuralCooperativeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChronicDiseaseServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.ChronicDiseaseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PolicyServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.PolicyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PayStandardServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.PayStandardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PayRecordServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.PayRecordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicalCardServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.MedicalCardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicalExpenseServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.MedicalExpenseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicalServlet</servlet-name>
        <servlet-class>com.gxuwz.medical_2.web.servlet.MedicalServlet</servlet-class>
    </servlet>
    <!--mapping-->
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoleServlet</servlet-name>
        <url-pattern>/system/RoleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MenuServlet</servlet-name>
        <url-pattern>/system/MenuServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/system/UserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AreaServlet</servlet-name>
        <url-pattern>/system/AreaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FamilyArchivesServlet</servlet-name>
        <url-pattern>/system/FamilyArchivesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountArchivesServlet</servlet-name>
        <url-pattern>/system/AccountArchivesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RuralCooperativeServlet</servlet-name>
        <url-pattern>/system/RuralCooperativeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChronicDiseaseServlet</servlet-name>
        <url-pattern>/system/ChronicDiseaseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PolicyServlet</servlet-name>
        <url-pattern>/system/PolicyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PayStandardServlet</servlet-name>
        <url-pattern>/system/PayStandardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PayRecordServlet</servlet-name>
        <url-pattern>/system/PayRecordServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicalCardServlet</servlet-name>
        <url-pattern>/system/MedicalCardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicalExpenseServlet</servlet-name>
        <url-pattern>/system/MedicalExpenseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicalServlet</servlet-name>
        <url-pattern>/system/MedicalServlet</url-pattern>
    </servlet-mapping>
</web-app>